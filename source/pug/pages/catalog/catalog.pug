extends ../../../layout/layout

include components/catalog-list

block varaiables
  - const meta = pages.catalog.meta
  - const {navigationLinks} = pages.catalog.data
  - const {page} = pages.catalog

block page-styles
  link(href!=`css/${page}.css` rel='stylesheet')

block page-head-scripts
  script(src!=`js/${page}-page-head.min.js`)

block main
  .container
    h1= page
    +catalog-list

block page-body-scripts
  script(src!=`js/${page}-page-body.min.js`)
