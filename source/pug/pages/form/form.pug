extends ../../layout

block varaiables
  - const meta = pages.form.meta
  - const {navigationLinks} = pages.form.data
  - const {page} = pages.form

block page-styles
  link(href!=`css/${page}.css` rel='stylesheet')

block page-head-scripts
  script(src!=`js/${page}-page-head.min.js`)

block main
  .container
    h1= page

block page-body-scripts
  script(src!=`js/${page}-page-body.min.js`)
